{
  "scopeName": "source.embedded.lang-injection",
  "injectionSelector": "L:source.lang",
  "patterns": [
     {
      "name": "meta.embedded.block.ts",
      "begin": "\\{ts:",
      "beginCaptures": {
        "0": { "name": "punctuation.section.embedded.begin.ts" }
      },
      "end": "\\s*:\\}",
      "endCaptures": {
        "0": { "name": "punctuation.section.embedded.end.ts" }
      },
      "contentName": "ts.embedded",
      "patterns": [
        {
          "include": "source.ts"
        }
      ]
    }
  ]
}
